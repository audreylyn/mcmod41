{
  "title": "MCiSmartSpace Documentation",
  "description": "Comprehensive user guides and technical documentation for Meycauayan College's SmartSpace room management system",
  "author": "Meycauayan College IT Department",
  "language": "en",
  "gitbook": ">=3.0.0",
  "root": ".",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "search",
    "sharing",
    "fontsettings",
    "theme-default",
    "livereload",
    "prism",
    "github",
    "edit-link",
    "page-toc-button",
    "back-to-top-button",
    "expandable-chapters",
    "copy-code-button",
    "alerts"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/meycauayan-college/smartspace"
    },
    "edit-link": {
      "base": "https://github.com/meycauayan-college/smartspace/edit/main/docs",
      "label": "Edit This Page"
    },
    "sharing": {
      "facebook": false,
      "twitter": false,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": ["print"]
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    }
  },
  "variables": {
    "systemName": "MCiSmartSpace",
    "collegeName": "Meycauayan College",
    "version": "2.0",
    "lastUpdated": "October 2025"
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}
